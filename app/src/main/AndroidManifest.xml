<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.win7.myapplication"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ParcelableActivity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings">
        </activity>
        <activity
            android:name="com.zaaach.citypicker.CityPickerActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustPan">
        </activity>

        <!-- 高德地图定位 -->
        <service android:name="com.amap.api.location.APSService"/>

        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="07e4e3deba98311f4e9749f8170a9166"/>
        <!-- 高德地图定位结束 -->
        <activity
            android:name=".SwipeDismissBehaviorActivity"
            android:label="@string/title_activity_main22"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>
